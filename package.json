{
  "name": "react-webpack-mastery",
  "version": "1.0.0",
  "description": "A hands-on learning project that explores React and Webpack, providing a practical and educational experience for mastering modern web development.",
  "main": "src/index.js",
  "scripts": {
    "analyze": "pnpm build:prod analyze",
    "build:prod": "pnpm build --env appEnv=prod",
    "build": "webpack --config webpack/webpack.prod.js",
    "start:dev": "pnpm start --env appEnv=dev",
    "start": "webpack serve --config webpack/webpack.dev.js"
  },
  "author": "Abhishek Rawat",
  "license": "MIT",
  "devDependencies": {
    "@types/react": "^18.2.13",
    "@types/react-dom": "^18.2.6",
    "css-loader": "^6.8.1",
    "css-minimizer-webpack-plugin": "^5.0.0",
    "html-webpack-plugin": "^5.5.1",
    "mini-css-extract-plugin": "^2.7.6",
    "sass": "^1.63.3",
    "sass-loader": "^13.3.2",
    "style-loader": "^3.3.3",
    "terser-webpack-plugin": "^5.3.9",
    "ts-loader": "^9.4.3",
    "typescript": "^5.1.3",
    "webpack": "^5.85.0",
    "webpack-bundle-analyzer": "^4.9.0",
    "webpack-cli": "^5.1.1",
    "webpack-dev-server": "^4.15.1",
    "webpack-merge": "^5.9.0"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
}
